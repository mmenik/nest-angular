{
  "name": "nest-angular",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nest": "nest",
    "start": "run-s build watch:server",
    "start:watch": "run-p build:watch watch:server",
    "start:ng": "ng serve",
    "start:nest": "nest serve",
    "watch:server": "nodemon --watch ./server/src --exec ts-node --inspect ./server/src/main.ts",
    "watch:client": "ng serve --proxy-config proxy.conf.json",
    "watch": "run-p watch:*",
    "build": "ng build --prod",
    "build:watch": "ng build --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
